<script>
import anime from 'animejs';

const animations = [
    {
        targets: '.zoom-out',
        scale: 1.3,
        autoplay: true,
        delay: 500, 
        duration: 1500, 
        easing: 'easeOutExpo', 
        direction: 'alternate',
    },
    {
        targets: '.fade-right',
        translateX: [-150, 0],
        opacity: [0, 1],
        autoplay: true,
        delay: 1000, 
        duration: 2000, 
        easing: 'easeInOutQuad', 
    },
    {
        targets: '.fade-left',
        translateX: [150, 0],
        opacity: [0, 1],
        autoplay: true,
        delay: 1500, 
        duration: 2000, 
        easing: 'easeInOutQuad', 
    },
    {
        targets: '.fade-down',
        translateY: [-150, 0],
        opacity: [0, 1],
        autoplay: true,
        delay: 2000, 
        duration: 2500, 
        easing: 'easeInOutQuart', 
    },
    {
        targets: '.fade-up',
        translateY: [150, 0],
        opacity: [0, 1], 
        autoplay: true,
        delay: 2500, 
        duration: 2500, 
        easing: 'easeInOutQuart', 
    }
];


    animations.forEach(animation => {
        if (document.querySelector(animation.targets)) {
            anime(animation);
        }
    });

    // var animasi = document.querySelector('section');
    // console.log(animasi);

</script>
