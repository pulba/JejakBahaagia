<script>
    import anime from 'animejs';
    anime({
        targets: '.zoom-out',
        scale: 1.3,
        autoplay: true,
        Delay: 3000,
        duration: 2000,
        easing: 'easeInOutSine',
        direction: 'alternate'
    });
    anime({
        targets: '.fade-right',
        translateX: [-150, 0],
        autoplay: true,
        Delay: 3000,
        duration: 2000,
        easing: 'easeInOutSine',
    });
    anime({
        targets: '.fade-left',
        translateX: [150, 0],
        autoplay: true,
        Delay: 3000,
        duration: 2000,
        easing: 'easeInOutSine',
    });
    anime({
        targets: '.fade-down',
        translateY: [-150, 0],
        autoplay: true,
        duration: 3000,
        easing: 'easeInOutSine',
    });
    anime({
        targets: '.fade-up',
        translateY: [150, 0],
        autoplay: true,
        duration: 3000,
        easing: 'easeInOutSine',
    });

    
</script>