            style={`background-image:url(${frontmatter.imgsc})`}>
            </div>
            <img src=`${frontmatter.dekorth}` alt="JejakBahagia"  class="absolute">
        </div>
      <div
        class=`animate-fadeleft flex flex-col sm:flex-row gap-2 text-center font-montecarlo text-center text-6xl lg:text-8xl ${frontmatter.txsc}`>
        <span>{frontmatter.female }</span>
        <span class=`${frontmatter.txth}`> & </span> 
        <span> {frontmatter.male } </span> 
      </div>
      <div class=`text-center ${frontmatter.txth}`>
