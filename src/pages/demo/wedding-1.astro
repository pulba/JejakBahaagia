      <div class="absolute overflow-hidden z-20 w-full h-[100%] *:absolute *:object-cover *:w-1/3 *:lg:w-1/5">
        <div>
          <img src=`${frontmatter.dekorfs}` class="" alt="JejakBahagia" />
        </div>
        <div class="right-0 top-0">
          <img src=`${frontmatter.dekorsc}` class="rotate-90 right-0 top-0" alt="JejakBahagia" />
        </div>
        <div class="left-0 bottom-0">
          <img src=`${frontmatter.dekorsc}` class="rotate-90 -scale-100" alt="JejakBahagia" />
        </div>
        <div class="bottom-0 right-0">
          <img src=`${frontmatter.dekorfs}` class="rotate-180" alt="JejakBahagia" />
        </div>
      </div>
      <div
        class="container mx-auto h-screen flex flex-col justify-center items-center relative z-10 font-josefin"
      >
        <h2 class=`animate-fadedown text-xl mt-4 ${frontmatter.txth}`>The Wedding of</h2>
        <div
        class=`animate-fadeleft flex flex-col sm:flex-row gap-2 text-center font-montecarlo text-center text-5xl sm:text-6xl lg:text-8xl ${frontmatter.txsc}`>
        <span>{frontmatter.female }</span>
        <span> & </span> 
        <span> {frontmatter.male } </span> 
      </div>
        <h3 class=`animate-fadeup text-xl md:text-2xl ${frontmatter.txsc}`>{frontmatter.date }</h3>
      </div>
    </section>
    <section class="animasi2 dh-3/6 pb-20 overflow-hidden">
      <div class="container mx-auto">
        <div class=`flex flex-col text-center items-center ${frontmatter.txsc}`>
