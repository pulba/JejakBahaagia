  frontmatter=data;
}


---
<Noheader title={frontmatter.title} desc={frontmatter.desc}>
    
  <input type="checkbox" id="toggle" class="peer hidden peer-checked:transition-all peer-checked:duration-700 peer-checked:ease-linear"/>
  <section class=`animasi11 relative h-screen bg-cover bg-center overflow-hidden flex items-center peer-checked:h-0 peer-checked:transition-all peer-checked:duration-700 peer-checked:ease-linear peer-checked:delay-500 before:content-[''] before:bg-gradient-to-t  ${frontmatter.from} before:to-transparent before:inset-0 before:bg-cover before:absolute`
    style={`background-image:url(${frontmatter.cover})`}>
    <div class="absolute overflow-hidden z-20 w-full h-[100%] *:absolute *:object-cover *:w-1/3 *:lg:w-1/5">
      <div>
        <img src=`${frontmatter.dekorfs}` class="" alt="JejakBahagia" />
      </div>
      <div class="right-0 top-0">
        <img src=`${frontmatter.dekorsc}` class="rotate-90 right-0 top-0" alt="JejakBahagia" />
      </div>
      <div class="left-0 bottom-0">
        <img src=`${frontmatter.dekorsc}` class="rotate-90 -scale-100" alt="JejakBahagia" />
      </div>
      <div class="bottom-0 right-0">
        <img src=`${frontmatter.dekorfs}` class="rotate-180" alt="JejakBahagia" />
      </div>
    </div>
