---
import Noheader from '../../layouts/noheader.astro';
import '@fontsource/montecarlo';
import '@fontsource-variable/josefin-sans';
import {Icon } from 'astro-iconify';
import Namatamu from '../../layouts/namatamu.astro'
import Payment from '../../layouts/payment.astro'
import Rsvp from '../../layouts/rsvp.astro'
import Countdown from '../../layouts/countdown.astro'
import Footerpost from '../../layouts/footer-post.astro'
import Audio from '../../layouts/audio.astro'
import Animation from '../../layouts/audio.astro'

const {frontmatter} = Astro.props
---

<Noheader title={frontmatter.title} desc={frontmatter.desc}>
  
  <input type="checkbox" id="toggle" class="peer hidden peer-checked:transition-all peer-checked:duration-700 peer-checked:ease-linear"/>
  <section class="animasi relative h-screen bg-cover bg-center flex items-center overflow-hidden peer-checked:h-0 peer-checked:transition-all peer-checked:duration-700 peer-checked:ease-linear peer-checked:delay-500"
    style={`background-image:url(${frontmatter.cover})`}>
    <div class="absolute overflow-hidden z-20 w-full h-[100%] [&>*]:absolute [&>*]:object-cover [&>*]:w-1/2 [&>*]:lg:w-1/5">
        <div>
            <img src=`${frontmatter.dekorfs}` class="" alt="JejakBahagia" />
        </div>
        <div class="right-0 top-0">
            <img src=`${frontmatter.dekorsc}` class="rotate-90 right-0 top-0" alt="JejakBahagia" />
        </div>
        <div class="left-0 bottom-0">
            <img src=`${frontmatter.dekorsc}` class="rotate-90 -scale-100" alt="JejakBahagia" />
        </div>
        <div class="bottom-0 right-0">
            <img src=`${frontmatter.dekorfs}` class="rotate-180" alt="JejakBahagia" />
        </div>
    </div>
    <div class="container mx-auto flex flex-col justify-center items-center text-white relative overflow-hidden z-50">
        <h4 class=`animate-fadedown text-xl mb-8 font-josefin font-semibold ${frontmatter.txsc}`>
            Wedding Invitation
        </h4>
        <div class="relative overflow-hidden"></div>
        <div class="animate-zoomin relative">
            <div class="relative flex items-center justify-center w-56 md:w-64">
                <div class="w-48 h-48 bg-cover bg-center relative"
                    style={`background-image:url(${frontmatter.imgsc}); clip-path: polygon(50% 0%, 100% 25%, 100% 75%, 50% 100%, 0% 75%, 0% 25%);`}>
                </div>
                <img src=`${frontmatter.dekorth}` alt="JejakBahagia" class="absolute">
            </div>
        </div>
        <div class="mx-auto text-center">
            <div
                class=`animate-zoomout flex flex-col sm:flex-row gap-2 text-center font-montecarlo text-center text-6xl lg:text-8xl ${frontmatter.txsc}`>
                <span>{frontmatter.female }</span>
                <span> & </span>
                <span> {frontmatter.male } </span>
            </div>
            <div class=`${frontmatter.txth}`>
                <h3 class="animate-faderight mt-2 font-poppins">Dear Mr./Mrs./Ms.</h3>
                <h4 id="invite-name" class="animate-fadeleft font-bold text-xl my-3 capitalize">
                    <Namatamu /> nama tamu
                </h4>
            </div>
        </div>
        <label for="toggle" id="toaudio"
            class=`animate-fadeup mt-5 px-6 py-2 relative rounded-full shadow-xl font-bold flex gap-1 ${frontmatter.txfs} ${frontmatter.bgsc}`>
            Open Invitation <Icon name="mdi:love-letter" class="w-5 h-5" />
        </label>
    </div>
</section>

  <main id="myDIV" class="hidden bg-blue-100 peer-checked:block">
    <section class="animasi1 relative h-screen bg-cover bg-center mx-auto overflow-hidden" style={`background-image:url(${frontmatter.cover})`}>
      <div class="absolute overflow-hidden z-20 w-full h-[100%] [&>*]:absolute [&>*]:object-cover [&>*]:w-1/2 [&>*]:lg:w-1/5">
        <div>
          <img src=`${frontmatter.dekorfs}` class="" alt="JejakBahagia" />
        </div>
        <div class="right-0 top-0">
          <img src=`${frontmatter.dekorsc}` class="rotate-90 right-0 top-0" alt="JejakBahagia" />
        </div>
      </div>
      <div class="container mx-auto h-screen flex flex-col justify-center items-center text-white relative z-10 font-josefin">
        <div class="mx-auto text-center flex flex-col items-center">
          <img src=`${frontmatter.dekorfv}` alt="JejakBahagia" class="animate-fadedown" />
          <h2 class=`animate-faderight text-xl mt-4 ${frontmatter.txth}`>The Wedding of</h2>
          <div class=`animate-zoomout flex flex-col sm:flex-row gap-2 text-center font-montecarlo text-center text-6xl lg:text-8xl ${frontmatter.txsc}`>
            <span>{frontmatter.female}</span>
            <span> & </span>
            <span>{frontmatter.male}</span>
          </div>
          <h3 class=`animate-fadeup text-xl md:text-2xl ${frontmatter.txsc}`>{frontmatter.date}</h3>
        </div>
      </div>
    </section>
    
    <section class="animasi2 h-max overflow-hidden relative">
      <div class="absolute z-20 w-full h-[100%] [&>*]:absolute [&>*]:object-cover [&>*]:w-1/2 [&>*]:lg:w-1/5">
        <div>
          <img src=`${frontmatter.dekorfs}` class="" alt="JejakBahagia" />
        </div>
        <div class="right-0 top-0">
          <img src=`${frontmatter.dekorsc}` class="rotate-90 right-0 top-0" alt="JejakBahagia" />
        </div>
      </div>
      <div class="container pt-20 px-6 mx-auto">
        <div class="flex flex-col text-center items-center">
          <h2 class="fade-down text-center font-montecarlo text-blue-600 text-6xl lg:text-8xl my-6">
            Quote
          </h2>
          <p class="fade-up opacity-0 sm:max-w-prose px-5 sm:px-0 max-w-none text-sm font-josefin">
            {frontmatter.quote}
          </p>
        </div>
        <div class="flex flex-col text-center items-center mt-20">
          <img src=`${frontmatter.dekorfr}` class="zoom-in opacity-0 w-full max-w-64" alt="JejakBahagia">
        </div>
      </div>
    </section>    
    <section class="animasi4 relative overflow-hidden">
      <div class="absolute z-20 w-full h-[100%] [&>*]:absolute [&>*]:object-cover [&>*]:w-1/2 [&>*]:lg:w-1/5">
        <div class="left-0 bottom-0 -rotate-90">
          <img src=`${frontmatter.dekorsc}` class="" alt="JejakBahagia" />
        </div>
        <div class="right-0 bottom-0 -rotate-180">
          <img src=`${frontmatter.dekorfs}` class="" alt="JejakBahagia" />
        </div>
      </div>
      <div class="container py-20 mx-auto relative z-50">
        <div class=`fade-down flex flex-col text-center items-center mb-9 ${frontmatter.txsc}`>
          <h1 class="fade-down opacity-0 text-center font-montecarlo text-6xl lg:text-8xl">
            Bride x Groom
          </h1>
          <p class="fade-right opacity-0 font-medium pb-8 text-lg font-josefin">
            We Are Getting Married
          </p>
        </div>
        <div class="mx-auto p-4">
        <div class="flex flex-col lg:flex-row justify-center items-center gap-11 gap-y-11 max-w-4xl mx-auto [&>*]:text-center">
          <div class="w-full flex flex-col items-center">
            <div class="fade-right opacity-0 relative flex items-center justify-center w-72">
              <div
                class="rounded-full w-60 h-60 bg-cover bg-center bg-white flex justify-center items-center"
                style={`background-image:url(${frontmatter.pm1})`}
              ></div>
              <img src={`${frontmatter.dekorth}`} alt="JejakBahagia" class="absolute">
            </div>
            <h3 class={`fade-down opacity-0 text-8xl mt-6 font-nautigal ${frontmatter.txsc}`}>{frontmatter.female}</h3>
            <div class="fade-up opacity-0 font-poppins space-y-3">
              <div class={`${frontmatter.txth}`}>
                <p class={`text-xl font-medium w-max mx-auto border-b-2 ${frontmatter.bdr}`}>{frontmatter.fullfemale}</p>
                <p class="text-lg">{frontmatter.childfemale}</p>
                <p class={`text-lg font-medium ${frontmatter.txsc}`}>{frontmatter.parentfemale}</p>
                <p>{frontmatter.cityfemale}</p>
                <div class="flex text-center justify-center mt-4">
                  <a href={`https://www.instagram.com/${frontmatter.instagramfemale}`} class="flex flex-row gap-2 items-center">
                    <Icon name="mdi:instagram" class={`w-10 h-10 p-2 rounded-full ${frontmatter.txfs} ${frontmatter.bgsc}`} />
                  </a>
                </div>
              </div>
            </div>
          </div>          
          <div class="w-full flex flex-col items-center">
            <div class="fade-left opacity-0 relative flex items-center justify-center w-72">
              <div
                class="rounded-full w-60 h-60 bg-cover bg-center bg-white flex justify-center items-center"
                style={`background-image:url(${frontmatter.pm2})`}
              ></div>
              <img src={`${frontmatter.dekorth}`} alt="JejakBahagia" class="absolute">
            </div>
            <h3 class={`fade-down opacity-0 text-8xl mt-6 font-nautigal ${frontmatter.txsc}`}>{frontmatter.male}</h3>
            <div class="fade-up opacity-0 font-josefin space-y-3">
              <div class={`${frontmatter.txth}`}>
                <p class={`text-xl font-medium w-max mx-auto border-b-2 ${frontmatter.bdr}`}>{frontmatter.fullmale}</p>
                <p class="text-lg">{frontmatter.childmale}</p>
                <p class={`text-lg font-medium ${frontmatter.txsc}`}>{frontmatter.parentmale}</p>
                <p>{frontmatter.citymale}</p>
                <div class="flex text-center justify-center mt-4">
                  <a href={`https://www.instagram.com/${frontmatter.instagrammale}`} class="flex flex-row gap-2 items-center">
                    <Icon name="mdi:instagram" class={`w-10 h-10 p-2 rounded-full ${frontmatter.txfs} ${frontmatter.bgsc}`} />
                  </a>
                </div>
              </div>
            </div>
          </div>
          </div>
        </div>
      </div>
    </section>
    <section class=`animasi5 relative overflow-hidden bg-cover bg-center bg-fixed bg-center before:content-[''] before:bg-gradient-to-b before:to-transparent before:inset-0 before:bg-cover before:absolute ${frontmatter.from}`
    style={`background-image:url(${frontmatter.add1})`}>
      <div class="container py-32 mx-auto relative z-10">
        <div class=`text-center my-5 ${frontmatter.txsc}`>
          <h2 class="fade-down opacity-0 text-xl md:text-2xl font-bold mt-5 md:mt-0 mb-3">Countdown to</h2>
          <h1 class="fade-down opacity-0 text-center font-montecarlo text-4xl md:text-6xl lg:text-8xl">
            Our Happy Day
          </h1>
        </div>
        <div id="timer" class=`fade-up opacity-0  grid grid-cols-2 md:grid-cols-4 gap-y-8 justify-center text-center items-center mt-20 sm:px-20 max-w-3xl mx-auto [&>*]:text-center [&>*]:text-4xl [&>*]:sm:text-6xl [&>*]:font-medium [&>*]:p-1 ${frontmatter.txsc}`>
        </div>
        <Countdown/>
      </div>
    </section>
    <section class="animasi7 overflow-hidden relative">
      <div class="absolute overflow-hidden z-20 w-full h-[100%] [&>*]:absolute [&>*]:object-cover [&>*]:w-1/2 [&>*]:lg:w-1/5">
        <div>
          <img src=`${frontmatter.dekorsc}` class="" alt="JejakBahagia" />
        </div>
        <div class="right-0 top-0">
          <img src=`${frontmatter.dekorsc}` class="rotate-90 right-0 top-0" alt="JejakBahagia" />
