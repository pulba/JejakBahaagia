<Noheader
  title={title}
  desc="Ini halaman Demo Wedding 2 dari undangan"
>
<input type="checkbox" id="toggle" class="peer hidden peer-checked:transition-all peer-checked:duration-700 peer-checked:ease-linear " />
<section class="hero relative h-screen bg-cover bg-center peer-checked:h-0 peer-checked:transition-all peer-checked:duration-700 peer-checked:ease-linear peer-checked:delay-500">
  <div class="absolute inset-0 bg-gray-500 bg-cover bg-center grayscale" style={`background-image:url(${cover})`}></div>
  
  <div class="container mx-auto pt-20 flex flex-col justify-center items-center text-white relative">
    <h1 class="fade-down text-sm md:text-xl mt-8 font-poppins font-semibold text-gray-800">
      Undangan Pernikahan
    </h1>
    <h2 class="fade-right text-center font-nautigal font-thin text-white max-[300px]:text-5xl min-[300px]:text-6xl md:text-7xl lg:text-8xl drop-shadow-xl">{ male } & { female }
    </h2>
    <div class="fade-up mx-auto text-center">
      <h3 class="mt-2 font-poppins">Dear Mr./Mrs./Ms.</h3>
      <h4 id="invite-name" class="text-xl font-poppins my-3 capitalize">
        <Namatamu /> nama tamu
      </h4>
      <p class="font-poppins text-xl md:text-2xl font-bold">{date}</p>
      <label
        for="toggle"
        class="mt-5 px-6 py-2 relative bg-gray-400 rounded-full shadow-xl text-gray-800 font-bold flex flex-row items-center justify-center gap-1 transition-all duration-700 ease-linear hover:bg-white"><Icon name="mdi:love-letter" class="w-5 h-5" /> Open Invitation </label>
    </div>
  </div>
</section>
<main id="myDIV" class="block bg-gray-100 peer-checked:block scroll-smooth">
  <section class="hero relative h-screen">
    <div class="absolute inset-0 bg-gray-500 bg-cover bg-center grayscale" style={`background-image:url(${imgth})`}></div>
    
    <div class="fade-down container mx-auto pt-20 flex flex-col justify-center items-center text-white relative z-50">
      <h2 class="text-center font-nautigal font-thin text-white max-[300px]:text-5xl min-[300px]:text-6xl md:text-7xl lg:text-9xl drop-shadow-xl">{ male } & { female }
      </h2>
      <p class="fade-right font-poppins text-xl md:text-2xl mt-5 font-bold">{date}</p>
    </div>
  </section>
  <section>
    <div class="container py-20 px-6 mx-auto">
      <div class="flex flex-col md:flex-row gap-16 max-w-4xl mx-auto my-12">
        <div class="fade-right rounded-lg grayscale">
          <img src={imgth} alt=""  class="rounded-3xl w-screen">
        </div>
        <div class="fade-left flex flex-col items-center justify-center font-poppins text-center">
          <img src="/src/assets/pattern.png" alt="">
          <p>{quote}</p>
        </div>
      </div>
    </div>
  </section>

